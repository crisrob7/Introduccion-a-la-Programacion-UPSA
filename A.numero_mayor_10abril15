#include "stdafx.h"
#include"stdafx.h"
#include<iostream.h>
#include"conio.h"
using namespace std;

int main() {

	int n,i;cantidad,mayor;
	cout << "Cuantos numeros va a ingresar: " << endl;
	cin >> cantidad;
	mayor = 0;
	for(i=1+1,i<cantidad;i++)

		cout << "Ingrese los numeros que desee: " << endl;
	    cin >> n;
		if(mayor<n){
			mayor=n;
		}

	cout << "El numero mayor es: " << endl;
	cin >> mayor;

	 getch();

}
